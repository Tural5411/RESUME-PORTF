<!--
===================
   BLOG
===================
-->
<section class="mh-blog" id="mh-blog">
    <div class="container">
        <div class="row section-separator">
            <div class="col-sm-12 section-title wow fadeInUp" data-wow-duration="0.8s" data-wow-delay="0.2s">
                <h3>Featured Posts</h3>
            </div>
            @foreach (tbl_Post ps in Model.Posts)
            {
                <div class="col-sm-12 col-md-4">
                    <div class="mh-blog-item dark-bg wow fadeInUp" data-wow-duration="0.8s" data-wow-delay="0.3s">
                        <img src="@ps.Photo" alt="" class="img-fluid">
                        @*assets/images/b-3.png*@
                        <div class="blog-inner">
                            <h2><a href="blog-single.html">@ps.Header</a></h2>
                            <div class="mh-blog-post-info">
                                <ul>
                                    <li><strong></strong><a href="#">@ps.Header2</a></li>

                                </ul>
                            </div>
                            <p> @ps.Description</p>
                            <a href="@Url.Action("PostDetail", "Home" , new { id= ps.Id})">@ps.Button</a>
                        </div>
                    </div>
                </div>

            }

        </div>
    </div>
</section>

<!--
===================
   TESTIMONIALS
===================
-->
<section class="mh-testimonial" id="mh-testimonial">
    <div class="home-v-img">
        <div class="container">
            <div class="row section-separator">
                <div class="col-sm-12 section-title wow fadeInUp" data-wow-duration="0.8s" data-wow-delay="0.2s">
                    <h3>Client Reviews</h3>
                </div>
                <div class="col-sm-12 wow fadeInUp" id="mh-client-review" data-wow-duration="0.8s" data-wow-delay="0.3s">
                    @foreach (tbl_Client cl in Model.Clients)
                    {
                        <div class="each-client-item">
                            <div class="mh-client-item dark-bg black-shadow-1">
                                <img src="@cl.Photo" alt="" class="img-fluid img-thumbnail">

                                <p>
                                    @cl.Description
                                </p>
                                <h4>@cl.FullName</h4>
                                <span>@cl.Firm</span>
                            </div>
                        </div>

                    }

                </div>
            </div>
        </div>
    </div>
</section>

